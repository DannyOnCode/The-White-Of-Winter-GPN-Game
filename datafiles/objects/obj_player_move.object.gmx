    {
        
        if(hspeed &lt; -6 &amp;&amp; !(hspeed &gt; -6)){
           hspeed = -6 
        }else{
            sprite_index = spr_player_walk
        if(speed &lt; 3) image_speed = 0.2
        if(speed &lt; 6) image_speed = 0.4
        hspeed -= Speed
        image_xscale = -1
        }
    }


//Move Right
if(key_right) &amp;&amp; (place_free(x + Speed,y) &amp;&amp; !dashing &amp;&amp;!sticking)
    {
        if(hspeed &gt; 6) {
            hspeed = 6
        }
        else{
            sprite_index = spr_player_walk
            if(speed &lt; 3) image_speed = 0.2
            if(speed &lt; 6) image_speed = 0.4
            
            hspeed += Speed
            image_xscale = 1
        }
    }

//Idle
if(speed = 0)
    {image_speed = 0
        image_index = 3
    }

    

